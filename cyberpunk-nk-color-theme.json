{
  "name": "Cyberpunk NK",
  "type": "dark",
  "colors": {
    "editor.background": "#212121",
    "editor.foreground": "#cacaca",
    "editorLineNumber.foreground": "#676767",
    "editorLineNumber.activeForeground": "#f3505c",
    "editorCursor.foreground": "#cacaca",
    "editor.selectionBackground": "#3e4452",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editorIndentGuide.background": "#676767",
    "editorIndentGuide.activeBackground": "#f3505c",
    "editorWhitespace.foreground": "#777777",
    "editorBracketMatch.background": "#262626",
    "editorBracketMatch.border": "#f3505c",
    "editorGutter.background": "#212121",
    "editorGutter.modifiedBackground": "#ffcc00",
    "editorGutter.addedBackground": "#51f66f",
    "editorGutter.deletedBackground": "#f3505c",
    "sideBar.background": "#2a2a2a",
    "sideBar.foreground": "#cacaca",
    "sideBarSectionHeader.background": "#323232",
    "sideBarSectionHeader.foreground": "#cacaca",
    "activityBar.background": "#2a2a2a",
    "activityBar.foreground": "#f3505c",
    "activityBarBadge.background": "#f3505c",
    "activityBarBadge.foreground": "#212121",
    "statusBar.background": "#2a2a2a",
    "statusBar.foreground": "#cacaca",
    "statusBar.noFolderBackground": "#262626",
    "tab.activeBackground": "#212121",
    "tab.inactiveBackground": "#2a2a2a",
    "tab.activeForeground": "#00f0ff",
    "tab.inactiveForeground": "#676767",
    "tab.border": "#f3505c",
    "panel.background": "#2a2a2a",
    "panel.border": "#f3505c",
    "titleBar.activeBackground": "#212121",
    "titleBar.activeForeground": "#cacaca",
    "titleBar.inactiveBackground": "#262626",
    "titleBar.inactiveForeground": "#777777",
    "notification.background": "#2a2a2a",
    "notification.foreground": "#cacaca",
    "notification.buttonBackground": "#f3505c",
    "notification.buttonForeground": "#212121",
    "badge.background": "#f3505c",
    "badge.foreground": "#212121",
    "input.background": "#262626",
    "input.foreground": "#cacaca",
    "dropdown.background": "#262626",
    "dropdown.foreground": "#cacaca",
    "list.activeSelectionBackground": "#3e4452",
    "list.activeSelectionForeground": "#cacaca",
    "list.inactiveSelectionBackground": "#2a2a2a",
    "list.inactiveSelectionForeground": "#cacaca",
    "list.hoverBackground": "#323232",
    "list.hoverForeground": "#cacaca",
    "peekViewEditor.background": "#262626",
    "peekViewResult.background": "#2a2a2a",
    "peekViewTitle.background": "#212121"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#777777", "fontStyle": "italic" }
    },
    { "scope": ["constant"], "settings": { "foreground": "#d867c6" } },
    { "scope": ["string", "constant.character.escape"], "settings": { "foreground": "#eedb85" } },
    { "scope": ["variable"], "settings": { "foreground": "#cacaca" } },
    { "scope": ["keyword", "storage.type"], "settings": { "foreground": "#f3505c" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#00f0ff" } },
    { "scope": ["entity.name.type", "support.type"], "settings": { "foreground": "#51f66f" } },
    { "scope": ["entity.name.class", "entity.name.struct"], "settings": { "foreground": "#51f66f" } },
    { "scope": ["variable.parameter"], "settings": { "foreground": "#cacaca" } },
    { "scope": ["entity.name.tag"], "settings": { "foreground": "#368aec" } },
    { "scope": ["support.constant"], "settings": { "foreground": "#ffcc00" } },
    { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#603336", "background": "#f3505c" } }
  ]
}
